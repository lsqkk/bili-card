{
    "functions": {
        "api/card.js": {
            "maxDuration": 10,
            "memory": 1024,
            "runtime": "nodejs24.x"
        }
    },
    "routes": [
        {
            "src": "/api/card",
            "dest": "/api/card.js",
            "methods": [
                "GET"
            ]
        }
    ],
    "headers": [
        {
            "source": "/api/card",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET"
                }
            ]
        }
    ]
}